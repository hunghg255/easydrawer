{
  "name": "easy-draw",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "lint-fix": "npm run lint -- --fix",
    "start": "esno src/index.ts",
    "test": "vitest",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "changelog": "npx changeloggithub@latest --all true && git add CHANGELOG.md && git commit -m 'chore: update CHANGELOG.md'",
    "release": "bumpp -r && npm run changelog",
    "playground": "pnpm --dir ./playground run dev"
  },
  "author": "hunghg255",
  "devDependencies": {
    "@hunghg255/eslint-config": "^0.0.16",
    "@types/node": "^18.19.76",
    "bumpp": "^9.11.1",
    "esno": "^0.16.3",
    "git-scm-hooks": "^0.0.7",
    "prettier": "^3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "verify-commit-msg": "^0.0.10",
    "vitest": "^0.34.6"
  },
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  },
  "keywords": [],
  "homepage": "https://github.com/hunghg255/unbuild-package",
  "repository": {
    "type": "git",
    "url": "https://github.com/hunghg255/unbuild-package"
  },
  "bugs": {
    "url": "https://github.com/hunghg255/unbuild-package/issues"
  },
  "dependencies": {
    "@melloware/coloris": "^0.24.0",
    "bezier-js": "^6.1.4",
    "lodash-es": "^4.17.21"
  }
}
