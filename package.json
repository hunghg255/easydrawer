{
  "name": "easy-draw",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/styles.css",
      "require": "./dist/styles.css"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json",
    "./*": "./dist/*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "NODE_ENV=production tsup && pnpm run build:css",
    "build:css": "postcss ./src/styles/styles.tailwind.css -o ./src/styles/assets/styles.css && cp -R src/styles/assets/styles.css dist/styles.css",
    "prepublishOnly": "npm run build",
    "lint": "eslint src",
    "lint-fix": "npm run lint -- --fix",
    "start": "esno src/index.ts",
    "test": "vitest",
    "verify-commit": "verify-commit-msg",
    "prepare": "git-scm-hooks",
    "changelog": "npx changeloggithub@latest --all true && git add CHANGELOG.md && git commit -m 'chore: update CHANGELOG.md'",
    "release": "bumpp -r && npm run changelog",
    "dev:tsup": "NODE_ENV=development tsup --watch",
    "dev:playground": "pnpm --dir ./playground run dev",
    "dev:css": "postcss ./src/styles/styles.tailwind.css -o ./src/styles/assets/styles.css --watch",
    "dev": "npm-run-all --parallel dev:css dev:tsup dev:playground"
  },
  "author": "hunghg255",
  "devDependencies": {
    "@hunghg255/eslint-config": "^0.0.16",
    "@types/node": "^18.19.76",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.11.1",
    "esno": "^0.16.3",
    "git-scm-hooks": "^0.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.5.1",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "verify-commit-msg": "^0.0.10",
    "vitest": "^0.34.6"
  },
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  },
  "keywords": [],
  "homepage": "https://github.com/hunghg255/unbuild-package",
  "repository": {
    "type": "git",
    "url": "https://github.com/hunghg255/unbuild-package"
  },
  "bugs": {
    "url": "https://github.com/hunghg255/unbuild-package/issues"
  },
  "dependencies": {
    "@melloware/coloris": "^0.24.0",
    "bezier-js": "^6.1.4",
    "lodash-es": "^4.17.21"
  },
  "packageManager": "pnpm@8.15.9+sha512.499434c9d8fdd1a2794ebf4552b3b25c0a633abcee5bb15e7b5de90f32f47b513aca98cd5cfd001c31f0db454bc3804edccd578501e4ca293a6816166bbd9f81"
}
